{{#extends "base"}}
  {{#block "content"}}
    <h1>Products</h1>

    <table class="table">
      <tr>
        <th>Name</th>
        <th>Price</th>
      </tr>

      {{#each products}}
        <tr>
          <td>{{name}}</td>
          <td>{{price}}</td>
        </tr>
      {{/each}}

    </table>

  {{/block}}
{{/extends}}